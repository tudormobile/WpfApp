{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appFaviconPath": "images/logo.svg",
      "_appLogoPath": "images/logo.svg",
      "_appName": "WpfApp",
      "_appTitle": "Tudormobile.WpfApp",
      "_appFooter": "<span>Copyright (C) Tudornobile LLC</span>",
      "_enableSearch": false,
      "pdf": false,
      "_disableContribution": true
    }
  }
}